{
  "name": "Postgres-RDS-Connector",
  "config": {
    "value.converter.schema.enable": "true",
    "time.precision.mode": "connect",
    "name": "Postgres-RDS-Connector",
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "1",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "transforms": "convertTS, convertTimestamp",
    "errors.tolerance": "all",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true",
    "topics": "rds-snowflake-data",
    "transforms.convertTS.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.convertTS.target.type": "Timestamp",
    "transforms.convertTS.field": "timestamp",
    "transforms.convertTS.format": "yyyy-MM-dd'T'HH:mm:ss.SSSXXX",
    "transforms.convertTimestamp.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.convertTimestamp.target.type": "Timestamp",
    "transforms.convertTimestamp.field": "timestamp_clone_usage",
    "transforms.convertTimestamp.format": "yyyy-MM-dd'T'HH:mm:ss.SSSXXX",
    "connection.url": "jdbc:postgresql://qalatest.cluster-ch9wln6nnyws.us-west-2.rds.amazonaws.com/qalatest_db?ssl=true&sslmode=require&currentSchema=pulse",
    "connection.user": "informatica",
    "connection.password": "************",
    "dialect.name": "PostgreSqlDatabaseDialect",
    "insert.mode": "upsert",
    "table.name.format": "pulse.nusights_kaleidoscope_storage_container_view1_latest",
    "pk.mode": "record_value",
    "pk.fields": "container_uuid",
    "auto.create": "true",
    "auto.evolve": "true"
  }
}